# Ignore environment and secrets
.env
*.env
*.key
*.json
service-accounts/

# Ignore Python cache
__pycache__/
*.pyc
*.pyo

# Ignore editor and OS junk
.vscode/
*.DS_Store

# Ignore log files and runtime
*.log
*.sqlite3
*.db

# Ignore test or dev builds
build/
dist/
*.egg-info/

# Ignore Jupyter notebooks (optional)
*.ipynb

# Ignore coverage/testing
.coverage
htmlcov/
*.cover

# Ignore Docker-specific
*.tar
*.img
*.bak
